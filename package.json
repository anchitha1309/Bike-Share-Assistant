{
  "name": "bike-share-assistant",
  "version": "1.0.0",
  "description": "Natural language bike-share analytics assistant",
  "main": "dist/api/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/api/server.js",
    "dev:backend": "ts-node-dev --respawn --transpile-only src/api/server.ts",
    "dev:frontend": "vite --host 0.0.0.0 --port 5173",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "vite build",
    "test": "jest",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "bike-share",
    "analytics",
    "nlp",
    "sql",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "cors": "^2.8.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "pg": "^8.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "vite": "^6.3.5"
  }
}
